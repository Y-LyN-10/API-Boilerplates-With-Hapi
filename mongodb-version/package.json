{
  "name": "hapi-api-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for quick start API in Hapi.js",
  "repository": "http://gitlab.mentormate.bg/yulia.tenincheva/hapi-api-boilerplate",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "lab -c -v -r console -o stdout -r html -o test/coverage.html -r lcov -o lcov.info -r json -o test/data.json",
    "seed": "node -e \"require('./db/seeders/users').Seed();\"",
    "reset-db": "node -e \"require('./db/seeders/users').Reset();\"",
    "todo": "leasot --tags FIXME ./**/*.js ./**/**/*.js --ignore \"./node_modules/**/*.js\""
  },
  "keywords": [
    "Hapi",
    "API",
    "Boilerplate"
  ],
  "author": "Yulia Tenincheva",
  "license": "MIT",
  "devDependencies": {
    "blipp": "^2.3.0",
    "code": "^4.1.0",
    "hapi-swagger": "^7.7.0",
    "inert": "^4.1.0",
    "lab": "^12.1.0",
    "leasot": "^4.7.0",
    "vision": "^4.1.1"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "catbox": "^7.1.4",
    "catbox-memory": "^2.0.4",
    "catbox-redis": "^3.0.1",
    "dotenv": "^4.0.0",
    "dust": "^0.3.0",
    "dustjs-linkedin": "^2.7.5",
    "glue": "^4.1.0",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "hapi": "^16.4.3",
    "hapi-auth-google": "^16.1.0",
    "hapi-auth-jwt2": "^7.2.4",
    "hapi-dust": "^2.0.0",
    "hapi-mongo-models": "^6.0.0",
    "hapi-rate-limit": "^1.3.2",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.1",
    "labbable": "^2.1.1",
    "mongo-models": "^1.3.1",
    "mongodb": "^2.2.29",
    "nodemailer": "^4.0.1",
    "redis": "^2.7.1"
  }
}
